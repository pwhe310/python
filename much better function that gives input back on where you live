place_of_living = raw_input("Where do you live?")

def location(living):
    if living == "san fran" or living == "la" or living == "new york":
        return "\nYou live in the USA"
    elif living == "europe":
        return "\nYou live in Europe!"
    else:
        return "\nYou live in the middle of no where!"

print location(place_of_living)
